package stackpkg;
import java.util.*;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	   StackClass mystack = new StackClass();
        Scanner in = new Scanner(System.in);
        
        int n = in.nextInt();
        for(int i=0; i<n; i++){
            int a = in.nextInt();
            if(a == -1){
                System.out.println(mystack.pop()); 
            }
            else
            {
            	mystack.push(a);
            }
            }
        }

	}


